/*  home css ------- */

.tabBox .tabs {
    margin: 0;
    padding: 0 10px;
    overflow: hidden;
    height: 2.25em;
}

.tabBox .tabs li {
    float: left;
    list-style: none;
    margin: 0;
    padding: .25em .25em 0;
    height: 2em;
    overflow: hidden;
    position: relative;
    z-index: 1;
}

.hometab {
    float: left;
    height: 2em;
    line-height: 2em;
    -webkit-border-radius: 8px 8px 0 0;
    -moz-border-radius: 8px 8px 0 0;
    border-radius: 8px 8px 0 0;
    background: #FFF;
    border: 2px solid #F93;
    border-bottom: 0;
    padding: 0 10px;
    color: #999999;
    text-decoration: none;
}

.tabBox .tabs .select {
	top:-3px;
	height: 3em;
}

.tabBox .tabs .select a {
	color: #414141;
	border: 3px solid #F93;
    background: #FFF;
    -webkit-box-shadow: #CCC 0 0 .25em;
    -moz-box-shadow: #CCC 0 0 .25em;
    box-shadow: #CCC 0 0 .25em;
	
}

.tabBox .tabs a:hover {
    color: #666666;
    background: #fff;
	top:-3px;
}

.order-box{
	min-height:600px; 
	_height:600px;
	position: relative;
	padding: 25px;
	margin-bottom: 25px;	
	border: 4px solid #F93;
	background:#fff;
	-webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    -webkit-box-shadow: 2px 2px 3px #333333;
    -moz-box-shadow: 2px 2px 3px #333333;
    box-shadow: 2px 2px 3px #333333;
	z-index:2;
}

.coupons-table td, .coupons-table th {
    border: 1px solid #FFFFFF;
    padding: 10px;
}

.coupons-table .deal {
    text-align: left;
}

.coupons-table td {
    font-size: 12px;
}

.coupons-table a:hover {
    color:#f93;
}

.coupons-table .alt td, .coupons-table .alt th {
    background: none repeat scroll 0 0 #F1F1F1;
}

.coupons-table .deal-info .pic {
    padding-right: 10px;
    width: 75px;
}

.coupons-table td, .coupons-table th {
    text-align: center;
}

.coupons-table .deal-info td {
    background: none repeat scroll 0 0 transparent;
    border: medium none;
    padding: 0;
}

